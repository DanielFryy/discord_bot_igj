{
  "name": "discord_bot_igj",
  "version": "1.0.0",
  "description": "discord bot for my personal server called Gamers",
  "main": "build/bot.js",
  "scripts": {
    "start": "node ./src/bot.js",
    "dev": "nodemon ./src/bot.js",
    "test": "jest",
    "lint": "eslint './**/*.ts' --max-warnings 0",
    "check-format": "prettier -c './**/*.ts'",
    "format": "prettier -w ./**/*.ts",
    "check-types": "tsc",
    "build": "tsc src/bot.ts",
    "clean:build": "rm src/**/*.js && rm src/bot.js || echo project cleaned"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielFryy/discord_bot_igj.git"
  },
  "keywords": [
    "discord",
    "bot",
    "gamer",
    "danielfryy"
  ],
  "author": "Daniel Freire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielFryy/discord_bot_igj/issues"
  },
  "homepage": "https://github.com/DanielFryy/discord_bot_igj#readme",
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/node-cron": "^3.0.8",
    "@types/node-fetch": "^2.6.4",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
