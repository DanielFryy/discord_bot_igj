{
  "name": "discord_bot_igj",
  "version": "1.0.0",
  "description": "discord bot for my personal server called Gamers",
  "main": "build/bot.js",
  "scripts": {
    "start": "node ./build/bot.js || echo Missing build, run build script first",
    "test": "vitest",
    "lint": "eslint './**/*.ts' --max-warnings 0",
    "check-format": "prettier -c './**/*.ts'",
    "format": "prettier -w ./**/*.ts",
    "check-types": "tsc",
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "clean": "rm -rf node_modules && rm yarn.lock",
    "clean:build": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielFryy/discord_bot_igj.git"
  },
  "keywords": [
    "discord",
    "bot",
    "gamer",
    "danielfryy"
  ],
  "author": "Daniel Freire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielFryy/discord_bot_igj/issues"
  },
  "homepage": "https://github.com/DanielFryy/discord_bot_igj#readme",
  "dependencies": {
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.8",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "rollup": "^3.28.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
